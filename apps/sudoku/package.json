{
  "name": "@cynnix-studios/sudoku",
  "version": "0.0.0",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "dev": "expo start",
    "web": "expo start --web",
    "export:web": "corepack pnpm -w exec tsx scripts/generate-daily-static.ts --out apps/sudoku/public/puzzles/daily --daysPast 30 --daysFuture 370 --policyVersion v1 && node ../../scripts/verify-expo-public-env.mjs && expo export -p web --output-dir dist",
    "lint": "corepack pnpm exec eslint .",
    "typecheck": "corepack pnpm exec tsc -p tsconfig.json --noEmit",
    "test": "corepack pnpm exec jest",
    "test:e2e": "corepack pnpm exec playwright test -c e2e/playwright.config.ts",
    "verify:figma-make-snapshot": "node scripts/verify-figma-make-snapshot.mjs",
    "update:figma-make-manifest": "node scripts/update-figma-make-manifest.mjs"
  },
  "dependencies": {
    "@cynnix-studios/game-foundation": "workspace:*",
    "@cynnix-studios/sudoku-core": "workspace:*",
    "@cynnix-studios/supabase": "workspace:*",
    "@cynnix-studios/ui": "workspace:*",
    "@react-native-google-signin/google-signin": "^12.0.0",
    "@react-navigation/native": "^7.0.14",
    "expo": "~54.0.0",
    "expo-apple-authentication": "~7.1.2",
    "expo-blur": "~15.0.8",
    "expo-constants": "~18.0.7",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.0",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.0",
    "lucide-react-native": "^0.562.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.2.1",
    "react-native-safe-area-context": "5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@playwright/test": "^1.51.1",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.1.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.3"
  }
}
