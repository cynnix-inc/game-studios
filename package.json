{
  "name": "cynnix-studios-games",
  "private": true,
  "packageManager": "pnpm@9.15.2",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "dev:sudoku": "corepack pnpm -C apps/sudoku dev",
    "web:sudoku": "corepack pnpm -C apps/sudoku web",
    "export:web:sudoku": "corepack pnpm -C apps/sudoku export:web",
    "test:e2e:sudoku": "corepack pnpm -C apps/sudoku test:e2e",
    "supabase:start": "node scripts/supabase.mjs start --exclude logflare,vector",
    "supabase:stop": "node scripts/supabase.mjs stop",
    "supabase:status": "node scripts/supabase.mjs status",
    "supabase:reset": "node scripts/supabase.mjs db reset --yes",
    "supabase:gen:types": "node scripts/supabase-gen-types.mjs",
    "lint": "node scripts/turbo.mjs lint",
    "typecheck": "node scripts/turbo.mjs typecheck",
    "test": "node scripts/turbo.mjs test",
    "gen:freeplay-packs": "corepack pnpm exec tsx scripts/generate-freeplay-packs.ts",
    "format": "prettier -w ."
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^22.10.2",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^15.13.0",
    "prettier": "^3.4.2",
    "turbo": "^2.3.3",
    "typescript-eslint": "^8.26.0",
    "typescript": "^5.7.3",
    "tsx": "^4.19.2"
  },
  "pnpm": {
    "overrides": {
      "react": "19.1.0",
      "react-dom": "19.1.0",
      "@types/react": "19.1.0"
    }
  }
}


